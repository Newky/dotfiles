#git aliases
alias g='git'
alias gco="git checkout"
alias gcm="git commit"
alias gst="git status"
alias gstu="git status --untracked=no"
alias gadd="git add"
alias gpsh="git push origin"

#vim opens files in tabs by default
alias vim="vim -p"
alias ..="cd .."
alias lstr='ls -tr'

# ubuntu 12.04 specific
alias audiokill="pulseaudio -k && sudo alsa force-reload"
