set autoindent
set incsearch
set number
set pastetoggle=<F2>
set showcmd
set wildmenu
set wildmode=list:longest,full
nnoremap <silent> <C-t> :tabnew<CR>
nnoremap <silent> <C-Left> :tabprevious<CR>
nnoremap <silent> <C-Right> :tabnext<CR>
inoremap jj <Esc>
nnoremap <space> za
filetype plugin indent on
syntax on
